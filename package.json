{
  "name": "bycontract-plus",
  "version": "1.0.1",
  "description": "Extensions to byconvention library",
  "main": "index.js",
  "scripts": {
    "require+": "node -p 'const { validate, is } = require(\"./index\"); validate(\"str\", \"string\");'",
    "require-": "node -p 'const { validate, is } = require(\"./index\"); validate(\"str\", \"int\");'",
    "import+": "node -r esm -p 'import { validate, is } from \"./index\"; validate(\"str\", \"string\");'",
    "import-": "node -r esm -p 'import { validate, is } from \"./index\"; validate(\"str\", \"int\");'"
  },
  "keywords": [
    "validation",
    "parameter validation",
    "byextension"
  ],
  "author": "Blaine Simpson <blaine.simpson@admc.com>",
  "license": "MIT",
  "homepage": "https://github.com/unsaved/bycontract-plus",
  "repository": {
    "type": "git",
    "url": "https://github.com/unsaved/bycontract-plus.git"
  },
  "dependencies": {
    "bycontract": "^2.0.11",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "esm": "^3.2.25"
  }
}
